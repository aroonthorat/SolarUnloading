{
    "version": 2,
    "rewrites": [
        {
            "source": "/web_app/(.*)",
            "destination": "/web_app/$1"
        },
        {
            "source": "/",
            "destination": "/web_app/index.html"
        },
        {
            "source": "/((?!web_app/).*)",
            "destination": "/web_app/$1"
        }
    ]
}